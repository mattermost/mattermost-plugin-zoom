    "@mattermost/client": "10.8.0",
    "@mattermost/types": "10.8.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.4",
